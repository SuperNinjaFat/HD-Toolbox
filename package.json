{
  "name": "hd-toolbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "mkdirp public/build && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src-ui/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src-ui/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/drawer": "^6.0.0-beta.16",
    "@smui/layout-grid": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/paper": "^6.0.0-beta.16",
    "@smui/switch": "^6.0.0-beta.16",
    "@smui/tooltip": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@tauri-apps/api": "^1.0.0-rc.5",
    "@tsconfig/svelte": "^2.0.1",
    "mkdirp": "^1.0.4",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.0.0",
    "svelte-check": "^2.7.0",
    "svelte-material-ui": "^6.0.0-beta.16",
    "svelte-preprocess": "^4.10.6",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#dev",
    "tslib": "^2.0.0",
    "typescript": "^4.6.4"
  }
}
